<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx" fx:controller="com.company.TeamManagementController">
    <TableView fx:id="teamTable">
        <columns>
            <TableColumn fx:id="teamIdColumn" text="Team ID"/>
            <TableColumn fx:id="teamNameColumn" text="Team Name"/>
        </columns>
    </TableView>
    <Button text="Create Team" onAction="#handleCreateTeam"/>
    <Button text="Manage Projects" onAction="#handleManageProjects"/>
</VBox>
